{
    "name": "node-browser",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/mgmt/node-browser/src",
    "prefix": "contezza",
    "targets": {
        "build": {
            "executor": "@nrwl/angular:package",
            "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
            "options": {
                "project": "libs/mgmt/node-browser/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "libs/mgmt/node-browser/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "libs/mgmt/node-browser/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/mgmt/node-browser/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/mgmt/node-browser/**/*.ts", "libs/mgmt/node-browser/**/*.html"]
            }
        }
    },
    "tags": []
}
